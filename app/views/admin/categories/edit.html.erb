<h2>Edit Category</h2>
<%= form_for [:admin, @category] do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class:'form-control' %>
  </div>
  <%= f.submit "Submit", class:'btn btn-primary' %>
  <%= link_to "Cancel", admin_categories_path, class:'btn btn-default' %>
<% end %>